<flynt-component name="NavigationFooterColumns">
  <div class="container">
    {% if preContentHtml %}
      <div class="preContent">
        {{ preContentHtml|e('wp_kses_post') }}
      </div>
    {% endif %}
    {% if menu.items|length > 0 %}
      <nav class="navigation" aria-label="{{ labels.navigationAriaLabel }}">
        {% include 'Partials/_menu.twig' with {items: menu.items} %}
      </nav>
    {% endif %}
    </div>
</flynt-component>
